  <!-- ================= -->
  <xsd:complexType name="PhysicalTerminationPointList_T">
    <xsd:annotation>
      <xsd:documentation>
        <p>A list of PhysicalTerminationPoints.</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ptp" type="tns:PhysicalTerminationPoint_T" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ================= -->
  <xsd:complexType name="ConnectionTerminationPointList_T">
    <xsd:annotation>
      <xsd:documentation>
        <p>A list of ConnectionTerminationPoints.</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ctp" type="tns:ConnectionTerminationPoint_T" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ================= -->
  <xsd:complexType name="FloatingTerminationPointList_T">
    <xsd:annotation>
      <xsd:documentation>
        <p>A list of FloatinglTerminationPoints.</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="ftp" type="tns:FloatingTerminationPoint_T" minOccurs="0" maxOccurs="unbounded"/>
    </xsd:sequence>
  </xsd:complexType>
  <!-- ================= -->
  <xsd:complexType name="TerminationPoint_T">
    <xsd:annotation>
      <xsd:documentation>
        <p>A union containment of all forms of TerminationPoints (PTP, FTP, and CTP).</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:choice>
      <xsd:element name="ptp" type="tns:PhysicalTerminationPoint_T"/>
      <xsd:element name="ftp" type="tns:FloatingTerminationPoint_T"/>
      <xsd:element name="ctp" type="tns:ConnectionTerminationPoint_T"/>
    </xsd:choice>
  </xsd:complexType>
  <!-- ================= -->
  <xsd:complexType name="TerminationPointList_T">
    <xsd:annotation>
      <xsd:documentation>
        <p>A heterogeneous list of TerminationPoints (PTPs, FTPs, and CTPs) with no particular order.</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="tp" type="tns:TerminationPoint_T" minOccurs="0" maxOccurs="unbounded">
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
