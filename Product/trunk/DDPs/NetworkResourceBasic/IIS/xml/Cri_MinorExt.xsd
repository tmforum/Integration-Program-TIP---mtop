      <xsd:element name="layerColor" maxOccurs="unbounded">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="layer" type="lay:LayerRate_T">
            </xsd:element>
            <xsd:element name="color" type="tns:Color_T">
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="minorV" type="xsd:positiveInteger" fixed="3"/>
  </xsd:complexType>
