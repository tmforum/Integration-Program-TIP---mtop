    <!-- ================ -->
    <wsdl:operation name="getAllManagedElementNamesPassingFilter">
      <soap:operation style="document"/>
      <wsdl:input>
        <soap:header message="tns:getAllManagedElementNamesPassingFilterRequest" part="mtopHeader" use="literal"/>
        <soap:body parts="mtopBody" use="literal"/>
