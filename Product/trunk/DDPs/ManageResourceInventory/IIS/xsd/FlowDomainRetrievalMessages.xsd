  <!-- ================ -->
  <xsd:element name="getFlowDomainFragmentsWithTpRequest">
    <xsd:annotation>
      <xsd:documentation>
        <p>Request message structure of the getFlowDomainFragmentsWithTp operation</p>
        <p>This operation allows the OS to request from the EMS the flow domain fragments that are connected to a specified flow point or CPTP. In case of a flow point as input parameter not more then one flow domain fragment will be returned.</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="tpName" type="nam:NamingAttributeType">
          <xsd:annotation>
            <xsd:documentation>
              <p>The name of the flow point or CPTP for which the connected flow domain fragments shall be returned.</p>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="getFlowDomainFragmentsWithTpResponse">
    <xsd:annotation>
      <xsd:documentation>
        <p>Response message structure of the getFlowDomainFragmentsWithTp operation</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="fdfrList" type="fdfr:FlowDomainFragmentListType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              <p>Retrieved FdFrList with the termination point name as specified in input.</p>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="getFlowDomainFragmentsWithTpException">
    <xsd:annotation>
      <xsd:documentation>
        <p>Exception message structure of the getFlowDomainFragmentsWithTp operation</p>
      </xsd:documentation>
    </xsd:annotation>
