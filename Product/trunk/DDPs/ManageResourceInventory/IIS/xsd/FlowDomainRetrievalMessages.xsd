      <xsd:documentation>
        <p>Response message structure of the getFlowDomainFragmentsByUserLabel operation</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="fdfrList" type="fdfr:FlowDomainFragmentListType" minOccurs="0">
