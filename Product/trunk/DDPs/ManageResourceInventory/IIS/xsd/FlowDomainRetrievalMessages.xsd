  <!-- ================ -->
  <xsd:element name="getAllAssociatedMatrixFlowDomainsRequest">
    <xsd:annotation>
      <xsd:documentation>
        <p>Request message structure of the getAllAssociatedMatrixFlowDomains operation</p>
        <p>This operation allows an OS to request a list of matrix flow domains that are contained by a specified managed element or associated with a specified equipment, transmission descriptor or flow domain.</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="holderName" type="nam:NamingAttributeType">
          <xsd:annotation>
            <xsd:documentation>
              <p>Name of the managed element, equipment, transmission descriptor or flow domain.</p>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="getAllAssociatedMatrixFlowDomainsResponse">
    <xsd:annotation>
      <xsd:documentation>
        <p>Response message structure of the getAllAssociatedMatrixFlowDomains operation</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="mfdList" type="mfd:MatrixFlowDomainListType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              <p>Retrieved MatrixFlowDomains with the name as specified in input.</p>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="getAllAssociatedMatrixFlowDomainsException">
    <xsd:annotation>
      <xsd:documentation>
        <p>Exception message structure of the getAllAssociatedMatrixFlowDomains operation</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="msg:AllExceptionsType">
          <xsd:choice>
            <xsd:element ref="msg:communicationFailure"/>
            <xsd:element ref="msg:entityNotFound"/>
            <xsd:element ref="msg:internalError"/>
            <xsd:element ref="msg:invalidInput"/>
            <xsd:element ref="msg:notImplemented"/>
            <xsd:element ref="msg:tooManyOpenIterators"/>
          </xsd:choice>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <!-- ================ -->
  <xsd:element name="getAllFlowDomainFragmentsRequest">
    <xsd:annotation>
      <xsd:documentation>
        <p>Request message structure of the getAllFlowDomainFragments operation</p>
        <p>This operation an OS to request a list of flow domain fragments for the specified flow domain at the specified connectivityRates.</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="fdName" type="nam:NamingAttributeType">
          <xsd:annotation>
            <xsd:documentation>
              <p>Name of the flow domain</p>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="connectivityRateList" type="lay:LayerRateListType">
          <xsd:annotation>
            <xsd:documentation>
              <p>List of rates of the flow domain fragments to be reported. If an empty list is specified, then all flow domain fragments of all rates are to be reported</p>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="getAllFlowDomainFragmentsResponse">
    <xsd:annotation>
      <xsd:documentation>
        <p>Response message structure of the getAllFlowDomainFragments operation</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="fdfrList" type="fdfr:FlowDomainFragmentListType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              <p>Retrieved FdFrList with the name as specified in input.</p>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="getAllFlowDomainFragmentsException">
    <xsd:annotation>
      <xsd:documentation>
        <p>Exception message structure of the getAllFlowDomainFragments operation</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="msg:AllExceptionsType">
          <xsd:choice>
            <xsd:element ref="msg:communicationFailure"/>
            <xsd:element ref="msg:entityNotFound"/>
            <xsd:element ref="msg:internalError"/>
            <xsd:element ref="msg:invalidInput"/>
            <xsd:element ref="msg:notImplemented"/>
            <xsd:element ref="msg:tooManyOpenIterators"/>
          </xsd:choice>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <!-- ================ -->
  <xsd:element name="getAllFlowDomainsRequest">
    <xsd:annotation>
      <xsd:documentation>
        <p>Request message structure of the getAllFlowDomains operation</p>
        <p>This operation allows an OS to request a list of the flow domains that exist under the given MD..</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="mdName" type="nam:NamingAttributeType">
          <xsd:annotation>
            <xsd:documentation>
              <p>The name of the Management Domain whose contained FDs are being requested</p>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="getAllFlowDomainsResponse">
    <xsd:annotation>
      <xsd:documentation>
        <p>Response message structure of the getAllFlowDomains operation</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="fdList" type="fd:FdListType" minOccurs="0">
          <xsd:annotation>
            <xsd:documentation>
              <p>List of flowDomains.</p>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="getAllFlowDomainsException">
    <xsd:annotation>
      <xsd:documentation>
        <p>Exception message structure of the getAllFlowDomains operation</p>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:restriction base="msg:AllExceptionsType">
          <xsd:choice>
            <xsd:element ref="msg:communicationFailure"/>
            <xsd:element ref="msg:entityNotFound"/>
            <xsd:element ref="msg:internalError"/>
            <xsd:element ref="msg:invalidInput"/>
            <xsd:element ref="msg:notImplemented"/>
            <xsd:element ref="msg:tooManyOpenIterators"/>
          </xsd:choice>
        </xsd:restriction>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>
  <!-- ================ -->
