	<xsd:import namespace="http://www.tmforum.org/mtop/nrb/xsd/lp/v1" schemaLocation="../../../NetworkResourceBasic/IIS/xsd/LayeredParameters.xsd"/>
	<!-- ================ -->
	<xsd:element name="getAllAssociatedMatrixFlowDomainsRequest">
		<xsd:annotation>
			<xsd:documentation>
				<p>Request message structure of the getAllAssociatedMatrixFlowDomains operation</p>
				<p>This operation allows an OS to request a list of matrix flow domains that are contained by a specified managed element or associated with a specified equipment, transmission descriptor or flow domain.</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="fdOrTmdRef " type="nam:NamingAttributeType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>This parameter provides the name of the Flow Domain or Transmission Descriptor for which to retrieve the associated Matrix Flow Domains.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAllAssociatedMatrixFlowDomainsResponse">
		<xsd:annotation>
			<xsd:documentation>
				<p>Response message structure of the getAllAssociatedMatrixFlowDomains operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="mfdList" type="mfd:MatrixFlowDomainListType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>This parameter returns the list of all Matrix Flow Domain objects associated with the provided Flow Domain or Transmission Descriptor.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAllAssociatedMatrixFlowDomainsException">
		<xsd:annotation>
			<xsd:documentation>
				<p>Exception message structure of the getAllAssociatedMatrixFlowDomains operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="msg:AllExceptionsType">
					<xsd:choice>
						<xsd:element ref="msg:commLoss"/>
						<xsd:element ref="msg:entityNotFound"/>
						<xsd:element ref="msg:internalError"/>
						<xsd:element ref="msg:invalidInput"/>
						<xsd:element ref="msg:notImplemented"/>
						<xsd:element ref="msg:tooManyOpenIterators"/>
						<xsd:element ref="msg:unableToComply"/>
					</xsd:choice>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<!-- ================ -->
	<xsd:element name="getAllFlowDomainFragmentsRequest">
		<xsd:annotation>
			<xsd:documentation>
				<p>Request message structure of the getAllFlowDomainFragments operation</p>
				<p>This operation retrieves the details of all the Flow Domain Fragments (FDFrs) contained in the provide Flow Domain (FD) by using a filter that is based on the Connectivity Rates.</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="fdRef" type="nam:NamingAttributeType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>This parameter provides the name of the Flow Domain whose Flow Domain Fragments are in scope.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="connectivityRateList" type="lay:LayerRateListType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>List of rates of the flow domain fragments to be reported. If an empty list is specified, then all flow domain fragments of all rates are to be reported.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAllFlowDomainFragmentsResponse">
		<xsd:annotation>
			<xsd:documentation>
				<p>Response message structure of the getAllFlowDomainFragments operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="fdfrList" type="fdfr:FlowDomainFragmentListType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>This parameter returns the list of Flow Domain Fragment objects contained in the Flow Domain selected by the given Connectivity Rates.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAllFlowDomainFragmentsException">
		<xsd:annotation>
			<xsd:documentation>
				<p>Exception message structure of the getAllFlowDomainFragments operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="msg:AllExceptionsType">
					<xsd:choice>
						<xsd:element ref="msg:commLoss"/>
						<xsd:element ref="msg:entityNotFound"/>
						<xsd:element ref="msg:internalError"/>
						<xsd:element ref="msg:invalidInput"/>
						<xsd:element ref="msg:notImplemented"/>
						<xsd:element ref="msg:tooManyOpenIterators"/>
						<xsd:element ref="msg:unableToComply"/>
					</xsd:choice>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<!-- ================ -->
	<xsd:element name="getAllFlowDomainsRequest">
		<xsd:annotation>
			<xsd:documentation>
				<p>Request message structure of the getAllFlowDomains operation</p>
				<p>This operation allows an OS to request a list of the flow domains that exist under the given MD.</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="mdRef" type="nam:NamingAttributeType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>The name of the Management Domain whose contained FDs are being requested.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAllFlowDomainsResponse">
		<xsd:annotation>
			<xsd:documentation>
				<p>Response message structure of the getAllFlowDomains operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="fdList" type="fd:FlowDomainListType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>List of FlowDomains.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAllFlowDomainsException">
		<xsd:annotation>
			<xsd:documentation>
				<p>Exception message structure of the getAllFlowDomains operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="msg:AllExceptionsType">
					<xsd:choice>
						<xsd:element ref="msg:commLoss"/>
						<xsd:element ref="msg:entityNotFound"/>
						<xsd:element ref="msg:internalError"/>
						<xsd:element ref="msg:invalidInput"/>
						<xsd:element ref="msg:notImplemented"/>
						<xsd:element ref="msg:tooManyOpenIterators"/>
						<xsd:element ref="msg:unableToComply"/>
					</xsd:choice>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<!-- ================ -->
	<xsd:element name="getAllSupportedMatrixFlowDomainsRequest">
		<xsd:annotation>
			<xsd:documentation>
				<p>Request message structure of the getAllSupportedMatrixFlowDomains operation</p>
				<p>This operation retrieves the details of all Matrix Flow Domains (MFDs) that are supported by the provided Managed Element or Equipment.</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="meOrEquipmentRef" type="nam:NamingAttributeType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>This parameter provides the name of the Managed Element or Equipment for which to retrieve the supported Matrix Flow Domains.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAllSupportedMatrixFlowDomainsResponse">
		<xsd:annotation>
			<xsd:documentation>
				<p>Response message structure of the getAllSupportedMatrixFlowDomains operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="mfdList" type="mfd:MatrixFlowDomainListType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>This parameter returns the list of all Matrix Flow Domain objects supported by the provided Managed Element or Equipment.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAllSupportedMatrixFlowDomainsException">
		<xsd:annotation>
			<xsd:documentation>
				<p>Exception message structure of the getAllSupportedMatrixFlowDomains operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="msg:AllExceptionsType">
					<xsd:choice>
						<xsd:element ref="msg:commLoss"/>
						<xsd:element ref="msg:entityNotFound"/>
						<xsd:element ref="msg:internalError"/>
						<xsd:element ref="msg:invalidInput"/>
						<xsd:element ref="msg:notImplemented"/>
						<xsd:element ref="msg:tooManyOpenIterators"/>
						<xsd:element ref="msg:unableToComply"/>
					</xsd:choice>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<!-- ================ -->
	<xsd:element name="getAssigningMatrixFlowDomainRequest">
		<xsd:annotation>
			<xsd:documentation>
				<p>Request message structure of the getAssigningMatrixFlowDomain operation</p>
				<p>This operation returns the matrix flow domain to which is assigned a given "CPTP" termination point.</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="cptpRef" type="nam:NamingAttributeType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>Name of the CPTP termination point to which the MatrixFlowDomain is assigned.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAssigningMatrixFlowDomainResponse">
		<xsd:annotation>
			<xsd:documentation>
				<p>Response message structure of the getAssigningMatrixFlowDomain operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="mfd" type="mfd:MatrixFlowDomainType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>Matrix flow domain structure returned.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAssigningMatrixFlowDomainException">
		<xsd:annotation>
			<xsd:documentation>
				<p>Exception message structure of the getAssigningMatrixFlowDomain operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="msg:AllExceptionsType">
					<xsd:choice>
						<xsd:element ref="msg:commLoss"/>
						<xsd:element ref="msg:entityNotFound"/>
						<xsd:element ref="msg:internalError"/>
						<xsd:element ref="msg:invalidInput"/>
						<xsd:element ref="msg:notImplemented"/>
						<xsd:element ref="msg:unableToComply"/>
					</xsd:choice>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<!-- ================ -->
	<xsd:element name="getAssociatingFlowDomainRequest">
		<xsd:annotation>
			<xsd:documentation>
				<p>Request message structure of the getAssociatingFlowDomain operation</p>
				<p>This operation returns the flow domain structure for the (single) flow domain that the Matrix Flow Domain that is supplied as a parameter is associated to.</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="mfdRef" type="nam:NamingAttributeType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>Name of the MatrixFlowDomain.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAssociatingFlowDomainResponse">
		<xsd:annotation>
			<xsd:documentation>
				<p>Response message structure of the getAssociatingFlowDomain operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="fd" type="fd:FlowDomainType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>The flow domain that the Matrix Flow Domain is associated to.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getAssociatingFlowDomainException">
		<xsd:annotation>
			<xsd:documentation>
				<p>Exception message structure of the getAssociatingFlowDomain operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="msg:AllExceptionsType">
					<xsd:choice>
						<xsd:element ref="msg:commLoss"/>
						<xsd:element ref="msg:entityNotFound"/>
						<xsd:element ref="msg:internalError"/>
						<xsd:element ref="msg:invalidInput"/>
						<xsd:element ref="msg:notImplemented"/>
						<xsd:element ref="msg:unableToComply"/>
					</xsd:choice>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<!-- ================ -->
	<xsd:element name="getFlowDomainFragmentRequest">
		<xsd:annotation>
			<xsd:documentation>
				<p>Request message structure of the getFlowDomainFragment operation</p>
				<p>This operation returns the flow domain fragment structure for the flow domain fragment whose name is supplied as a parameter.</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="fdfrRef" type="nam:NamingAttributeType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>Name of the FDFr to retrieve.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getFlowDomainFragmentResponse">
		<xsd:annotation>
			<xsd:documentation>
				<p>Response message structure of the getFlowDomainFragment operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="fdfr" type="fdfr:FlowDomainFragmentType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>Retrieved FDFr with the name as specified in input.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getFlowDomainFragmentException">
		<xsd:annotation>
			<xsd:documentation>
				<p>Exception message structure of the getFlowDomainFragment operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="msg:AllExceptionsType">
					<xsd:choice>
						<xsd:element ref="msg:commLoss"/>
						<xsd:element ref="msg:entityNotFound"/>
						<xsd:element ref="msg:internalError"/>
						<xsd:element ref="msg:invalidInput"/>
						<xsd:element ref="msg:notImplemented"/>
						<xsd:element ref="msg:unableToComply"/>
					</xsd:choice>
				</xsd:restriction>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>
	<!-- ================ -->
	<xsd:element name="getFlowDomainFragmentRouteRequest">
		<xsd:annotation>
			<xsd:documentation>
				<p>Request message structure of the getFlowDomainFragmentRoute operation</p>
				<p>This operation returns the route for the FDFr whose name is specified as a parameter. It is only used for systems where the route is provisioned by the OS (e.g., it is not useful for Ethernet networks using GVRP).</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="fdfrRef" type="nam:NamingAttributeType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>The name of the FDFr.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getFlowDomainFragmentRouteResponse">
		<xsd:annotation>
			<xsd:documentation>
				<p>Response message structure of the getFlowDomainFragmentRoute operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="route" type="fdfrr:FlowDomainFragmentRouteType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>The route of the FDFr.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="getFlowDomainFragmentRouteException">
		<xsd:annotation>
			<xsd:documentation>
				<p>Exception message structure of the getFlowDomainFragmentRoute operation</p>
			</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:restriction base="msg:AllExceptionsType">
