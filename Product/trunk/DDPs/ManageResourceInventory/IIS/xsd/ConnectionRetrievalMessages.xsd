        </xsd:element>
        <xsd:element name="includeHigherOrderCCs" type="xsd:boolean">
          <xsd:annotation>
            <xsd:documentation>
              <p>Specifies whether the higher order CCs of other SNCs used to carry the queried SNC route have to be included in addition to the CCs of the queried SNC route.</p>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element name="additionalInfo" type="nam:NvsList_T">
