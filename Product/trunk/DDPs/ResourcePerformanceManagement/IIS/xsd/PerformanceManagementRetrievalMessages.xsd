			<xsd:sequence>
				<xsd:element name="pmParameterList" type="pmpar:PerformanceMonitoringParameterListType" minOccurs="0">
					<xsd:annotation>
						<xsd:documentation>
							<p>Contains the set of PM parameters that are supported by the ME referred to.</p>
						</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
