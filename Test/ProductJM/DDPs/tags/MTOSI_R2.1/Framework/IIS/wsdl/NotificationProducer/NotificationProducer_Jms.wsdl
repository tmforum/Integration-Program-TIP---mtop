<wsdl:definitions name="mTOP-NotificationConsumerV1" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://www.tmforum.org/mtop/framework/ws/notc/v1" targetNamespace="http://www.tmforum.org/mtop/framework/ws/notc/v1">
  <wsdl:documentation>
    <p>DDP: Framework</p>
    <p>Module: NotificationConsumer Service Interface WSDL Module</p>
    <p>Description: This module contains all the mTOP NotificationConsumer Service Interface WSDL SOAP/JMS binding definitions based on an MSG style port type (interface).</p>
    <p>Version: 1.0</p>
    <p>Change Policy: TBD.</p>
  </wsdl:documentation>
  <!-- ================ -->
  <wsdl:import namespace="http://www.tmforum.org/mtop/framework/ws/notc/v1" location="NotificationConsumer_Messages.wsdl"/>
  <!-- ================ -->
  <wsdl:service name="NotificationConsumerJms">
    <wsdl:port name="NotificationConsumerSoapJms" binding="tns:NotificationConsumerSoapJmsBinding">
      <wsdl:documentation>
        <p>Example of the NotificationConsumer WSDL Service definition as a deployment example of this mTOP Service Interface. It is expected to be modified to match the specific deployment configuration in a service provider's CCV.</p>
      </wsdl:documentation>
      <soap:address location="jms://aserver/mtosi/NotificationConsumerV1"/>
    </wsdl:port>
  </wsdl:service>
  <!-- ================ -->
  <wsdl:binding name="NotificationConsumerSoapJmsBinding" type="tns:NotificationConsumer_MSG">
    <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/jms"/>
    <wsdl:documentation>
      <p>SOAP JMS binding definitions of the operations of the NotificationConsumer_MSG portType (interface)</p>
    </wsdl:documentation>
    <!-- ================ -->
    <wsdl:operation name="notify">
      <soap:operation style="document"/>
      <wsdl:input>
        <soap:header message="tns:notify" part="mtopHeader" use="literal"/>
        <soap:body parts="mtopBody" use="literal"/>
      </wsdl:input>
    </wsdl:operation>
  </wsdl:binding>
  <!-- ================ -->
  <wsdl:portType name="NotificationConsumer_MSG">
    <wsdl:documentation>
      <p>The NotificationConsumer porttype (interface) for MSG (asynchronous) based client/server communication patterns</p>
    </wsdl:documentation>
    <!-- ================ -->
    <wsdl:operation name="notify">
      <wsdl:documentation>
        <p>This is the notify operation used to receive notifications from producer or broker.</p>
      </wsdl:documentation>
      <wsdl:input message="tns:notify"/>
    </wsdl:operation>
  </wsdl:portType>
</wsdl:definitions>
